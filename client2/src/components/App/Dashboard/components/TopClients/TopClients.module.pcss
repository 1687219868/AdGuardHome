.container {
    display: flex;
    flex-flow: column;
    background-color: var(--white);
}

.title {
    font-size: 16px;
    line-height: 22px;
    margin-bottom: 4px;
    padding: 24px;
    color: var(--gray900);
    border-bottom: 1px solid var(--gray100);
}

.content {
    padding: 24px
}

.tableTitle {
    color: var(--gray400);
}

.tableGrid {
    display: grid;
    grid-template-columns: 4fr 1fr 1fr 1.5fr 1fr .5fr
}